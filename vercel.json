{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/error-test",
      "dest": "/index.html"
    },
    {
      "src": "/test-routes",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite"
} 